// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
    provider = "postgresql"
    url = "***"
}

generator client {
    provider = "prisma-client-js"
}

model Users {
    id           Int    @default(autoincrement()) @id
    email        String @unique
    firstName    String
    lastName     String
    phone        String
    password     String
    address      String @default("")
    age          Int    @default(0)
    tokenVersion Int    @default(1)
}
